{
  "class": "evaluation.listeners.MetricsGameListener",
  "args": [
    {
      "class" : "evaluation.loggers.FileStatsLogger",
      "args" : ["GameResults.txt"]
    },
    [
      {"class": "evaluation.metrics.GameMetrics$GameID", "args": [ [{"enum" : "evaluation.metrics.Event$GameEvent", "value" : "GAME_OVER"}]]},
      {"class": "evaluation.metrics.GameMetrics$GameName", "args": [ [{"enum" : "evaluation.metrics.Event$GameEvent", "value" : "GAME_OVER"}]]},
      {"class": "evaluation.metrics.GameMetrics$PlayerCount", "args": [ [{"enum" : "evaluation.metrics.Event$GameEvent", "value" : "GAME_OVER"}]]},
      {"class": "evaluation.metrics.GameMetrics$GameDuration" },
      {"class": "evaluation.metrics.GameMetrics$FinalScore" },
      {"class": "evaluation.metrics.GameMetrics$OrdinalPosition" },
      {"class": "evaluation.metrics.GameMetrics$PlayerType" }
    ]]
}